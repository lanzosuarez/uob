{
  "expo": {
    "sdkVersion": "29.0.0",
    "privacy": "unlisted",
    "version": "0.1.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "ios": {
      "bundleIdentifier": "com.uobsummit.app",
      "icon": "./assets/icon.png",
      "buildNumber": "19",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Allow uobsummit to access you location"
      }
    }
  }
}
